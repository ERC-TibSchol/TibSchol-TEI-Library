      <ab><milestone unit="section" n="Colophon (24b3–24b5)] "/> <rs type="work" ana="corpusmd:named_reference" ref="db:4459"><rs type="person" ana="corpusmd:unnamed_reference" ref="db:3786">bla ma rje btsun <lb n="24b4"/> dam pa <rs type="place" ana="corpusmd:named_reference" ref="db:4332">gsang phu</rs> lo tsha <anchor xml:id="a51"/> ba chen po</rs> <note type="gloss"><rs type="person" ana="corpusmd:named_reference" ref="db:3786">rngog blo ldan shes rab</rs></note> la bstod pa'i tshig le'ur <anchor xml:id="a52"/> byas pa'i rnam par bshad pa</rs> | <rs type="person" ana="corpusmd:named_reference" ref="db:3806">shAkya'i <anchor xml:id="a53"/> dge slong <note type="gloss">gro lung pa chen po</note> blo gros 'byung gnas</rs> kyis sbyar ba <lb n="24b5"/> rdzogs s+ho ||</ab>
      <gap reason="omitted"/>
      <ab><milestone unit="section" n="Colophon of dGe slong 'Phags pa (24b5–25a4)"/> bdag nyid chen po thugs legs par sbyangs pa'i me long yongs su dag <lb n="24b6"/> par <anchor xml:id="a55"/> legs par sbyar ba'i skad kyis rnam par sbyar ba'i gtsug lag gi tshogs mtha' yas pa kun du snang bas <rs type="place" ana="corpusmd:unnamed_reference" ref="db:5559">gangs ri 'i khrod</rs> na gnas <lb n="24b7"/> pa'i blo'i mig<anchor xml:id="a56"/> yongs su dag pa'i skye bo rnams la kun du<anchor xml:id="a57"/> ston par mdzad pa'i <rs type="work" ana="corpusmd:named_reference" ref="db:4459"><rs type="person" ana="corpusmd:named_reference" ref="db:3786">skad 2 smra ba chen po blo ldan shes rab</rs> kyi rnam par thar pa</rs> |<anchor xml:id="a58"/> de nyid <lb n="24b8"/> kyi sras kyi thu bo sdom brtson  <rs type="person" ana="corpusmd:named_reference" ref="db:3806">sde snod 'dzin pa chen po <note type="gloss"><rs type="person" ana="corpusmd:named_reference" ref="db:3806">gro lung pa</rs></note>blo gros 'byung gnas</rs> shes<anchor xml:id="a59"/> bya bas rnam par spel ba yod do zhes <anchor xml:id="a60"/> lung rin po che'i tshogs <lb n="25a1"/> kyis rnam par phyug pa <rs type="person" ana="corpusmd:named_reference" ref="db:4136"><abbr>slob dpon</abbr> dge ba'i bshes gnyen<note type="gloss">chag</note> nyi ma</rs>'i gsung las <rs type="person" ana="corpusmd:unnamed_reference" ref="db:5838">dge slong 'phags pa</rs>s gsal bar thos nas | <anchor xml:id="a61"/> gtam gyi skyes rmad <lb n="25a2"/> du byung ba de<anchor xml:id="a62"/> lta bu rna ba'i snod du nye bar lhung na ci ma rung snyam du bsams nas | <anchor xml:id="a63"/> ring du 'gro ba'i pho nya dag la nye bar 'gugs pa'i spying yig rnam <lb n="25a3"/> par springs pa'i don | <anchor xml:id="a64"/> <rs type="place" ana="corpusmd:named_reference" ref="db:4387">dpal rte'u ra'i gtsug lag khang</rs> na gnas pa'i <rs type="person" ana="corpusmd:unnamed_reference" ref="db:4593">dge ba'i bshes gnyen rnams</rs> kyis bsgrubs pa 'di <rs type="person" ana="corpusmd:unnamed_reference" ref="db:5838">bdag</rs> gi sug tu <lb n="25a4"/> thengs <anchor xml:id="a65"/> nas | <anchor xml:id="a66"/> lcags pho <anchor xml:id="a67"/> rta'i <anchor xml:id="a68"/> lo zla ba gcig pa la shin du <anchor xml:id="a69"/> dag par byas pa'o || dge'o ||</ab>
